    boxheight = (text_height/2) + (padding*2);
    
    
    draw_set_alpha(0.5);
    draw_set_font(font);
    draw_set_color(c_black);
    
    if (room != rmCredits){
        draw_rectangle(x+specialOffset, y + offset, x+boxwidth+specialOffset, y+boxheight + offset, 0);
    } else {
        draw_rectangle(x+specialOffset, y + offset, x+boxwidth+specialOffset, y+(boxheight+offset)/2-100, 0);
